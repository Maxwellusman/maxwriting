{
    "version": 2,
    "blob": {
      "allowedOrigins": [
        "https://maxwritings.com",
        "https://www.maxwritings.com",
        "http://localhost:3000"
      ],
      "restrictAccessToOrigins": true,
      "tokenDuration": "1d",
      "maxFileSize": "4MB"
    },
    "routes": [
      {
        "src": "/blog-images/(.*)",
        "dest": "/api/blob-proxy?path=$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://maxwritings.com"
          }
        ]
      }
    ]
  }